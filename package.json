{
  "name": "ncs-api-website",
  "private": true,
  "version": "1.0.0",
  "description": "NCS API Website - High-performance clustering visualization and API testing platform with real-time data processing capabilities",
  "type": "module",
  "main": "js/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "start": "npm run dev",
    "build:prod": "vite build --mode production",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "deploy": "npm run build && npm run preview"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "three": "^0.158.0",
    "papaparse": "^5.4.1",
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "eslint": "^8.57.0",
    "@vitejs/plugin-legacy": "^5.3.0"
  },
  "keywords": [
    "ncs",
    "clustering",
    "api",
    "vanilla-js",
    "three.js",
    "bootstrap",
    "machine-learning",
    "real-time",
    "data-processing",
    "data-visualization",
    "k-means",
    "dbscan",
    "hierarchical-clustering",
    "web-app",
    "pwa",
    "responsive-design"
  ],
  "author": {
    "name": "NCS API Team",
    "email": "contact@ncs-api.com",
    "url": "https://ncs-api.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelkatsweb/NCS-API-Website.git"
  },
  "bugs": {
    "url": "https://github.com/michaelkatsweb/NCS-API-Website/issues"
  },
  "homepage": "https://ncs-api.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "files": [
    "dist/**/*",
    "assets/**/*",
    "css/**/*",
    "js/**/*",
    "*.html",
    "manifest.json",
    "sw.js",
    "README.md"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/michaelkatsweb"
  },
  "config": {
    "port": 3000,
    "host": "localhost"
  }
}